<% content_for :title do %>User<% end %>
<h3>Contact</h3>
<div class="form">
  <%= form_with(model: @user) do |form| %>
    <%= form.label :name %>
    <%= form.text_field :name, autofocus: true %>
    <br/>
    <br/>
    <%= form.label :email %>
    <%= form.email_field :email %>
    <br/>
    <br/>
    <%= form.label :password %>
    <%= form.password_field :password %>
    <br/>
    <br/>
    <%= form.submit 'Submit', class: 'submit' %>
  <% end %>
</div>

